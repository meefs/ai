	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rm -rf dist && tsup --config tsup.config.ts",
		"format": "oxfmt --write .",
		"test:ci": "vitest --watch=false",
		"test": "vitest",
		"type-check": "tsc --noEmit"
	},
